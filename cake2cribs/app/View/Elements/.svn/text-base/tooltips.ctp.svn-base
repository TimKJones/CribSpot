<!-- Tooltip text hidden -->
<div style="display:none;">

	<!-- General Tooltip for Apartments, Houses and Duplexes-->
	<div id="generalTooltip">
		<div class="tooltip">
			<div id="addressRow">
				<a target="_blank"><div id="tooltipAddress" class="tooltipLabel"></div></a>
				<div class="backToMultipleListings" onclick="A2Cribs.Map.MarkerTooltip.CreateMultipleContent()"></div>
				<div id="addressRowButtons"><div id="addFavoriteImg"></div></div>
			</div>
			<div id="houseDataDiv">
				<div id="dataLeftColumn" class="tooltipLabel">
						Price:<br>Beds:<br>Baths:<br>Lease:<br>Type:<br>Furnished:<br>Realtor:<br>Utilites:<br>Parking:<br>A/C:
				</div>
				<div id="dataRightColumn">
					<span class="tooltipData" id="tooltipPrice"></span><br>
					<span class="tooltipData" id="tooltipBeds"></span><br>
					<span class="tooltipData" id="tooltipBaths"></span><br>
					<span class="tooltipData" id="tooltipLeaseRange"></span><br>
					<span class="tooltipData" id="tooltipType"></span><br>
					<span class="tooltipData" id="tooltipFurnished"></span><br>
					<a target="_blank" id="property_link"><span class="tooltipData" id="tooltipCompany"></span></a><br>
					<span class="tooltipData" id="tooltipUtilities"></span><br>
					<span class="tooltipData" id="tooltipParking"></span><br>
					<span class="tooltipData" id="tooltipAir"></span><br>
				</div>
			</div>
		</div>
	</div>

	<!-- Multi Bubble template for multiple listings on a single marker -->
	<div id="multiTooltip">
		<div class="tooltip">
			<div id="addressRow">
				<div id="tooltipAddress" class="tooltipLabel"></div> 	
			</div>
			<div id="multiBubbleContainer"></div>
		</div>
	</div>

</div>
