<div id="favoritesBar">
	<div id="favoriteActionsButton"></div>
	<div id="hideFavoritesDiv" onclick="HideFavorites()"></div>
	<div id="personalFavoritesList">
		<div id="noFavorites"></div>
	</div>


	<!-- Favorite Template -->
	<div id="favoriteTemplate" style="display:none;">
		<div id="favoriteDiv${ListingId}" class="favoriteDiv"> <!-- ID ==> favoriteDiv{listingID} -->
			<div class="favoritesAddress"></div> <!-- Address or alt name in bold -->
			<div class="removeButton"></div> <!-- Onclick delete favorite -->
			<div class="optionGrid">
				<div id="price" title="Rent for Entire Unit"></div>
				<div id="beds" title="Bedrooms">? Beds</div>
				<div id="furnished" title="Furnished"><div class="smallIcon"></div></div>
				<div id="parking" title="Parking"><div class="parking_unselected smallIcon"></div></div>
				<div id="ac" title="Air Conditioning"><div class="ac_unselected smallIcon"></div></div>
				<div id="payMonth" title="Lease Period">${LeaseRange}</div>
				<div id="aptType" title="Unit Type">${UnitType}</div>
				<div id="baths" title="Bathrooms">? Baths</div>
				<div id="electric" title="Electricity"><div class="electric_selected smallIcon"></div></div>
				<div id="heat" title="Heating"><div class="heat_unselected smallIcon"></div></div>
				<div id="water" title="Water"><div class="water_unselected smallIcon"></div></div>
				<div id="contact" title="Contact Information"><a target="_blank">Contact</a></div>
			</div>
		</div>
	</div>
	
</div>